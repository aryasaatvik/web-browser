{
  "name": "web-browser",
  "version": "0.1.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "MCP server and native messaging bridge for web-browser",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "web-browser": "./bin/web-browser.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.29",
    "@web-browser/core": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.1.0",
    "ai": "^6.0.58",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.1.3",
    "typescript": "^5.3.0",
    "vitest": "^3.1.3"
  }
}
